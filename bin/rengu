#!/usr/bin/python3

import sys

sys.path.append('')

from rengu.cmd import RenguCmd

if __name__ == '__main__':

    if len(sys.argv) > 1:
        RenguCmd().onecmd(' '.join(sys.argv[1:]))
    else:
        try:
            RenguCmd().cmdloop()
        except KeyboardInterrupt:
            print('\nexit')
